{{ define "content" }}
    <div class="title">
        <h1>Jacob Riddle</h1>
        <small>luckier than archer</small>
    </div>
    <div class="social">
        <a href="https://github.com/rudes" target="_blank">
            <i class="fa fa-github"></i>github
        </a>
        <a href="https://www.linkedin.com/in/jxriddle" target="_blank">
            <i class="fa fa-linkedin-square"></i>linkedin
        </a>
    </div>
    <div class="content">
        <div class="languages">
            Languages I Work In
            <table class="languages">
                {{ printLanguages }}
            </table>
        </div>
        <div class="projects">
            Projects I've Built
            <div class="row">
                <div class="img">
                    <a href="http://cryopaste.com">
                        <img alt="Cryo Paste Image" src="/static/images/cryopaste.png">
                    </a>
                </div>
                <small>Cryo Paste</small>
                <p>
                A command-line pastebin for quickly collaborating with friends
                </p>
            </div>
            <div class="row">
                <div class="img">
                    <a href="http://otherletters.net">
                        <img alt="Other Letter Image" src="/static/images/otherletters.png">
                    </a>
                </div>
                <small> Other Letters </small>
                <p>
                A blog written in Go and is fully customizable
                fork the project <a href="https://github.com/rudes/goblog">here</a>
                </p>
            </div>
            <div class="row">
                <div class="img">
                    <a href="http://runestats.stream">
                        <img alt="Rune Stats Image" src="http://runestats.stream/rudess.png"/>
                    </a>
                </div>
                <small> Rune Stats </small>
                <p>
                A live updating embeddable levels image for Oldschool RuneScape
                </p>
            </div>
        </div>
    </div>
{{ end }}
